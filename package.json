{
  "name": "stub-test-server",
  "version": "1.0.0",
  "description": "A dynamic stub test server for API endpoint simulation",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "NODE_ENV=test node --test tests/**/*.test.js"
  },
  "keywords": [
    "stub",
    "test",
    "server",
    "mock",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
